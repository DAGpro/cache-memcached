{
    "name": "yiisoft/cache-memcached",
    "type": "library",
    "description": "Yii Caching Library - Memcached Handler",
    "keywords": [
        "yii",
        "framework",
        "cache",
        "memcached",
        "psr-16"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/",
            "role": "Founder and former project lead"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/",
            "role": "Core framework development"
        }
    ],
    "support": {
        "source": "https://github.com/yiisoft/cache-memcached",
        "issues": "https://github.com/yiisoft/cache-memcached/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/cache": "^3.0@dev",
        "ext-memcached": "*"
    },
    "require-dev": {
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "phpunit/phpunit": "^7.3",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Cache\\Tests\\": [
                "vendor/yiisoft/cache/tests",
                "tests"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
